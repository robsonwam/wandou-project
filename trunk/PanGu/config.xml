<?xml version="1.0" encoding="utf-8"?>
<log>
<!-- inforamtion about Symbols -->

<Tokenizer name="logHeadTokenizer" value=","/>
<Tokenizer name="logBodyTokenizer" value=","/>
<Tokenizer name="logHeadBodyTokenizer" value=";"/>
<Tokenizer name="escapeSymbol" value="\"/>

<!-- inforamtion about log head -->
<loghead>
<tag name="TimeStamp" />
<tag name="RemoteIP" />
<tag name="OriginateIP" />
<tag name="UA" />
<tag name="SessionID" />
<tag name="TransactionID" />
<tag name="MobileNo" />
<tag name="Source" />
<tag name="ContentType" />
<tag name="Ptn" />
<tag name="Encoding" />
<tag name="Referer" />
<tag name="Uid" />
</loghead>

<logbody>
<!-- log body of product:image -->
<product name="image">
<!-- actinType:Click -->
<action name="imageclick">
<tag name="PageNum" />
<tag name="Query" />
<tag name="Rank" />
<tag name="Url" />
</action>

<!-- actinType:Search -->
<action name="imagesearch">
<tag name="Hits" />
<tag name="PageNum" />
<tag name="Query" />
<tag name="RespTime" />
<tag name="Start" />
</action>
</product>

<!-- log body of product:page -->
<product name="page">
<!-- actinType:Click -->
<action name="pageclick">
<tag name="PageNum" />
<tag name="Query" />
<tag name="Rank" />
<tag name="Url" />
</action>

<!-- actinType:Search -->
<action name="pagesearch">
<tag name="Hits" />
<tag name="PageNum" />
<tag name="Query" />
<tag name="RespTime" />
<tag name="Start" />
</action>

<!-- actinType:snapshot -->
<action name="pagesnapshot">
<tag name="Hits" />
<tag name="PageNum" />
<tag name="Query" />
<tag name="RespTime" />
<tag name="Start" />
</action>
</product>
</logbody>

</log>

